{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.17.1",
    "npm": "6.14.13"
  },
  "scripts": {
    "clean": "npm cache clean --force && rm -rf ./node_modules && npm install",
    "check": "npm install -g npm-check && npm-check",
    "test": "CI=true JEST_JUNIT_OUTPUT_DIR=../build/ui/report react-scripts test --watchAll=false --testResultsProcessor=\"jest-junit\" --coverage",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "1.1.2",
    "mdbreact": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "jest-junit": "^12.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
